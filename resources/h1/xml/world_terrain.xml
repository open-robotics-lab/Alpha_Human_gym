<mujoco>
	<include file="scene.xml"/>
	<include file="dependencies.xml"/>

	<worldbody>
		<include file="h1.xml" />
	</worldbody>
 	<actuator>      
		<motor name="J_L0"  	  joint="J_L0"   	 gear="1" ctrllimited="true" ctrlrange="-12 12"/>
		<motor name="J_L1"  	  joint="J_L1"  	 gear="1" ctrllimited="true" ctrlrange="-20 20"/>
		<motor name="J_L2"  	  joint="J_L2" 	 	 gear="1" ctrllimited="true" ctrlrange="-20 20"/>
		<motor name="J_L3"  	  joint="J_L3"  	 gear="1" ctrllimited="true" ctrlrange="-20 20"/>
		<motor name="J_L4_ankle"  joint="J_L4_ankle" gear="1" ctrllimited="true" ctrlrange="-12 12"/>
		
		<motor name="J_R0"  	  joint="J_R0"   	 gear="1" ctrllimited="true" ctrlrange="-12 12"/>
		<motor name="J_R1"  	  joint="J_R1"  	 gear="1" ctrllimited="true" ctrlrange="-20 20"/>
		<motor name="J_R2"  	  joint="J_R2" 		 gear="1" ctrllimited="true" ctrlrange="-20 20"/>
		<motor name="J_R3"  	  joint="J_R3"  	 gear="1" ctrllimited="true" ctrlrange="-20 20"/>
		<motor name="J_R4_ankle"  joint="J_R4_ankle" gear="1" ctrllimited="true" ctrlrange="-12 12"/>
    </actuator>

    <sensor>
    	<!--pos-->
		<actuatorpos name='J_L0_p' actuator='J_L0'  	 user='13'/>
		<actuatorpos name='J_L1_p' actuator='J_L1' 		 user='13'/>
		<actuatorpos name='J_L2_p' actuator='J_L2' 		 user='13'/>
		<actuatorpos name='J_L3_p' actuator='J_L3' 		 user='13'/>
		<actuatorpos name='J_L4_p' actuator='J_L4_ankle' user='13'/>
		
		<actuatorpos name='J_R0_p' actuator='J_R0'  	 user='13'/>
		<actuatorpos name='J_R1_p' actuator='J_R1' 		 user='13'/>
		<actuatorpos name='J_R2_p' actuator='J_R2' 		 user='13'/>
		<actuatorpos name='J_R3_p' actuator='J_R3' 		 user='13'/>
		<actuatorpos name='J_R4_p' actuator='J_R4_ankle' user='13'/>
		<!--vel-->
		<actuatorvel name='J_L0_v' actuator='J_L0'  	 user='13'/>
		<actuatorvel name='J_L1_v' actuator='J_L1' 		 user='13'/>
		<actuatorvel name='J_L2_v' actuator='J_L2'		 user='13'/>
		<actuatorvel name='J_L3_v' actuator='J_L3' 		 user='13'/>
		<actuatorvel name='J_L4_v' actuator='J_L4_ankle' user='13'/>
		
		<actuatorvel name='J_R0_v' actuator='J_R0'  	 user='13'/>
		<actuatorvel name='J_R1_v' actuator='J_R1' 	 	 user='13'/>
		<actuatorvel name='J_R2_v' actuator='J_R2'		 user='13'/>
		<actuatorvel name='J_R3_v' actuator='J_R3' 		 user='13'/>
		<actuatorvel name='J_R4_v' actuator='J_R4_ankle' user='13'/>
		<!--frc-->
		<actuatorfrc name='J_L0_f' actuator='J_L0'  	 user='13' noise='1e-3'/>
		<actuatorfrc name='J_L1_f' actuator='J_L1' 		 user='13' noise='1e-3'/>
		<actuatorfrc name='J_L2_f' actuator='J_L2'		 user='13' noise='1e-3'/>
		<actuatorfrc name='J_L3_f' actuator='J_L3' 		 user='13' noise='1e-3'/>
		<actuatorfrc name='J_L4_f' actuator='J_L4_ankle' user='13' noise='1e-3'/>
		
		<actuatorfrc name='J_R0_f' actuator='J_R0'  	 user='13' noise='1e-3'/>
		<actuatorfrc name='J_R1_f' actuator='J_R1' 		 user='13' noise='1e-3'/>
		<actuatorfrc name='J_R2_f' actuator='J_R2'		 user='13' noise='1e-3'/>
		<actuatorfrc name='J_R3_f' actuator='J_R3' 		 user='13' noise='1e-3'/>
		<actuatorfrc name='J_R4_f' actuator='J_R4_ankle' user='13' noise='1e-3'/>

        <framequat     name='orientation' objtype='site' noise='0.001' objname='imu'/>
        <framepos      name='position' 	  objtype='site' noise='0.001' objname='imu'/>
        <gyro          name='angular-velocity'    site='imu' noise='0.005' cutoff='34.9'/>
        <velocimeter   name='linear-velocity'     site='imu' noise='0.001' cutoff='30'/>
        <accelerometer name='linear-acceleration' site='imu' noise='0.005' cutoff='157'/>
        <magnetometer  name='magnetometer'        site='imu'/>
    </sensor>
	

</mujoco>
